@model IEnumerable<PortfolioCategoryDto>
@{
    //var newProductShowcase = ViewBag.NewProductShowcase as List<NewProductShocaseDto>;
    GeneralConfigurationDto conGen = JsonSerializer.Deserialize<GeneralConfigurationDto>(K["GeneralConfiguration"])!;
}

@if (Model != null && Model.Count() != 0)
{
    <div class="container my-5">
        <div>
            <span class="h-5 fw-700 d-block mb-2">Danh sách đối tác chấp nhận thanh toán quét mã QR của TicketVip.vn</span>
        </div>
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-lg col-md-4 col-sm-6 col-12">
                    <div class="mb-4">
                        <a href="#" class="text-decoration-none text-dark shadow-sm">
                            <div style="height: 300px;width: auto;">
                                <img alt="product" class="img-fluid border-radius-0" style="width: 100%;height: 100%;object-fit: cover;" src="https://vuonhoathanhphodalat.com.vn/wp-content/themes/tiendev/asset/images/logo.png" loading="lazy" decoding="async">
                            </div>
                            <div class="p-3 pt-2 height-100">
                                <span class="fw-600 d-block text-ellipsis--2 lh-sm mb-2">@item?.Name</span>
                                <span class="text-carrot fw-700 h-7">
                                    @Html.Raw(item?.Desciption)
                                </span>
                            </div>
                        </a>
                    </div>
                </div>
            }
        </div>
    </div>
}



